{
  "expo": {
    "name": "NotesApp",
    "slug": "notesapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/sunflower.png",
    "scheme": "com.jackferson.notesapp",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/sunflower.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "package": "com.jackferson.notesapp",
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/sunflower.png"
    }
  }
}
